{UTF-8}
ADULTOS= INTEG (
	madurez-catastrofe-muerte de adultos-VEJEZ,
		550000)
	~	personas
	~	55% de la población de 1000000 de personas
	|

ANCIANOS= INTEG (
	VEJEZ-muerte natural,
		120000)
	~	personas
	~	Si el 33% son jovenes y el 55 adultos, el resto son ancianos
	|

catastrofe=
	PULSE( 2018, 1)*150000
	~	personas
	~		|

JOVENES= INTEG (
	nacimientos-madurez-muerte de jovenes,
		330000)
	~	personas
	~		|

madurez=
	JOVENES/Periodo de madurez
	~	personas/año
	~		|

muerte de adultos=
	ADULTOS*tasa de mortalidad adultos
	~	personas/año
	~		|

muerte de jovenes=
	JOVENES*tasa de mortalidad jovenes
	~	personas/año
	~		|

muerte natural=
	ANCIANOS/Periodo final
	~	personas/año
	~		|

nacimientos=
	ADULTOS*tasa de natalidad
	~	personas/año
	~		|

Periodo de madurez=
	20
	~	año
	~		|

Periodo de vejez=
	51
	~	año
	~		|

Periodo final=
	9
	~	año
	~		|

poblacion total=
	JOVENES+ADULTOS+ANCIANOS
	~	personas
	~		|

tasa de mortalidad adultos=
	0.02
	~	1/año
	~		|

tasa de mortalidad jovenes=
	0.025
	~	1/año
	~		|

tasa de natalidad=
	0.06
	~	1/año
	~		|

VEJEZ=
	ADULTOS/Periodo de vejez
	~	personas/año
	~		|

