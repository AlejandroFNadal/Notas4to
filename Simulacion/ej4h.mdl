{UTF-8}
curacion=
	Infectada*tasaCuracion
	~	persona/dia
	~		|

infeccion=
	Susceptible*tasaContagio*Infectada
	~	persona/dia
	~		|

Infectada= INTEG (
	infeccion-curacion-muerte,
		100)
	~	persona
	~		|

muerte=
	Infectada*tasaMortalidad
	~	1/dia
	~		|

noInmune=
	Recuperada*tasaExposicion
	~	persona/dia
	~		|

Recuperada= INTEG (
	curacion+vacunacion-noInmune,
		0)
	~	persona
	~		|

Susceptible= INTEG (
	noInmune-infeccion-vacunacion,
		1000)
	~	persona
	~		|

tasaContagio=
	0.005
	~	1/persona
	~		|

tasaCuracion=
	0.01
	~	
	~		|

tasaExposicion=
	0.05
	~	
	~		|

tasaMortalidad=
	0.1
	~	
	~		|

tasaVacunacion=
	0.5
	~	1/dia
	~		|

vacunacion=
	Susceptible*tasaVacunacion
	~	persona/dia
	~		|

